  // Syntax colors
  "tokenColors": [
    {
      "name": "Blue",
      "scope": [
        "keyword",
        "meta.brace.round",
        "meta.brace.square",
        "meta.object-literal.key",
        "punctuation"
      ],
      "settings": {
        "foreground": "#7bacdd"
      }
    },
    {
      "name": "Bold",
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Gray",
      "scope": ["punctuation.terminator", "storage.modifier", "storage.type"],
      "settings": {
        "foreground": "#939393"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "keyword.control",
        "meta.object-literal.key",
        "storage.modifier",
        "storage.type",
        "variable.language.this"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Purple",
      "scope": ["entity.name.tag.html"],
      "settings": {
        "foreground": "#dda0dd"
      }
    },
    {
      "name": "Teal",
      "scope": [
        "entity.name.function",
        "meta.function",
        "meta.method.declaration"
      ],
      "settings": {
        "foreground": "#00f8fb"
      }
    },
    {
      "name": "Yellow",
      "scope": ["comment", "punctuation.definition.comment", "string"],
      "settings": {
        "foreground": "#d4b300"
      }
    },
    {
      "name": "White",
      "scope": ["constant.numeric", "support.class", "variable"],
      "settings": {
        "foreground": "#f2f1ef"
      }
    }
  ]
}
